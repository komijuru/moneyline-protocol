{
  "name": "moneyline-protocol",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@ethersproject/experimental": "^5.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.1.3",
    "@openzeppelin/contracts": "^4.8.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.3",
    "hardhat-deploy": "^0.11.20",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.8",
    "mocha": "^10.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-solidity": "^1.0.0",
    "solidity-coverage": "^0.8.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "run:local": "hardhat run --network localhost",
    "run:testnet": "hardhat run --network goerli",
    "run:mainnet": "ETH_NETWORK=mainnet hardhat run --network mainnet",
    "deploy:local": "hardhat deploy --network localhost --tags $tags --export ./deployments/localhost.json",
    "deploy:testnet": "hardhat deploy --network goerli --tags $tags --export ./deployments/goerli.json",
    "deploy:mainnet": "ETH_NETWORK=mainnet hardhat deploy --network mainnet --tags $tags --export ./deployments/mainnet.json",
    "verify:testnet": "hardhat --network goerli etherscan-verify",
    "verify:mainnet": "ETH_NETWORK=mainnet hardhat --network mainnet etherscan-verify",
    "export:testnet": "hardhat --network goerli export --export ./deployment/abi/goerli.json",
    "export:mainnet": "ETH_NETWORK=mainnet hardhat --network mainnet export --export ./deployment/abi/mainnet.json"
  }
}
